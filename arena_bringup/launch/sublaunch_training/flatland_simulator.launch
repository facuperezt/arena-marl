
<launch>
  <arg name="ns"/>
  <arg name="warehouse" default="false"/>

  <!-- launch flatland server -->
  <node name="flatland_server" pkg="flatland_server" type="flatland_server" output="screen" ns="$(arg ns)">  
    <!-- remapping for namespace -->
    <remap from="/clock" to="/$(arg ns)/clock"/>
  </node>

  <!-- <group if="$(eval arg('warehouse') == true)">
    <node name="warehouse_gen" pkg="warehouse" type="rviz_visualization.py" args="/$(arg ns)"/>
    <remap from="/warehouse_gen" to="$(arg ns)/warehouse_gen"/>
  </group> -->

</launch>